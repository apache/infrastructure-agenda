<!doctype html>
<html lang="en">
  [include "head.html.ezt"]
  <body>

  [include "navigation.html.ezt"]

  <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">[title]</h1><note>Total: [count]</note>
      </div>

      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Meeting Date</th>
              <th scope="col">Latest Revision</th>
              <th scope="col">Last Change Author</th>
              <th scope="col">Minutes</th>
            </tr>
          </thead>
          <tbody>
            [for items]
            <tr>
              <td><a href=[items.url]>[items.name]</a></td>
              <td>["%0 (%1)" items.revision items.rev_date_str]</td>
              <td>[items.revision_author]</td>
              <td><a href=[items.minutes_url]>[items.minutes]</a></td>
            </tr>
            [end]
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>


  [include "foot.html.ezt"]
  </body>
</html>
